	<queryString language="SQL">
		<![CDATA[SELECT ped.id, ped.estado, ped.f_emision, prov.nombre, ped.subtotal_usd, ped.total_usd, ped.f_confirmacion, ped.nro_factura
        FROM vam_pedidos AS ped
